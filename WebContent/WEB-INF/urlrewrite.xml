<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
  <rule>
  <from>/cpu_info</from>
  <to>/GetInfo</to>
 </rule>
 <rule>
 	<from>/cpu_rate=(/d+)$</from>
 	<to>/GetInfo?rate=$1</to>
 </rule>
</urlrewrite>